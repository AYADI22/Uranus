import telebot
import requests

# 1. ضع التوكن الخاص بك من BotFather هنا
TOKEN = "اكتب_التوكن_هنا"

# 2. مفتاح Qwen الخاص بك (الموجود في صورك السابقة)
OPENROUTER_KEY = "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"

bot = telebot.TeleBot(TOKEN)

@bot.message_handler(func=lambda message: True)
def handle_qwen(message):
    # الاتصال بموديل Qwen3 Coder المجاني
    response = requests.post(
        "https://openrouter.ai/api/v1/chat/completions",
        headers={"Authorization": f"Bearer {OPENROUTER_KEY}"},
        json={
            "model": "qwen/qwen3-coder:free",
            "messages": [{"role": "user", "content": message.text}]
        }
    )
    # إرسال الرد إلى تلجرام
    reply = response.json()['choices'][0]['message']['content']
    bot.reply_to(message, reply)

print("البوت بدأ العمل.. جربه في تلجرام الآن!")
bot.polling()
